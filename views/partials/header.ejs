    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css"> <!-- load fontawesome -->
    
    <style>
      body {
        padding-top: 80px;
        word-wrap: break-word;
      }
  
      .ccontainer {
        position: relative;
        display: flex;
        flex-direction: row;
        justify-content: center;
        align-items: center;
        height: 20rem;
        width: 60rem;
        background-color: transparent;
        border-radius: 3px;
        box-shadow: none;
      }
  
      .clock-column {
        margin-right: 7rem;
        text-align: center;
        position: relative;
  
        &::after {
          content: ':';
          display: block;
          height: .25rem;
          width: .25rem;
          font-size: 36px;
          font-weight: 200;
          color: #131313;
          position: absolute;
          top: 60px;
          right: -35px;
        }
  
        &:last-child {
          &::after {
            display: none;
          }
        }
      }
  
      .clock-column:last-child {
        margin-right: 0;
      }
  
      .clock-label {
        padding-top: 10px;
        text-transform: uppercase;
        color: #131313;
        font-size: 14px;
        text-align: center;
        border-top: 1px solid rgba(19, 19, 19, 0.5);
      }
  
      .clock-timer {
        color: #131313;
        font-size: 36px;
        line-height: 1;
      }
  
      .clock-input {
        clear: both;
         
        max-width: 500px;
        width: 100%;
        margin: 0 auto 10px;
      }
  
      input#time-to {
        padding: 5px;
        border: 0;
        border-radius: 3px;
        font-size: 16px;
        font-family: monospace;
        text-align: center;
        color: #010101;
        background-color: #fff;
      }
  
      .done {
        color: #fff;
        font-weight: 600;
      }
    </style>
  </head>

